        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/l_thorax"
            android:text="ABDOMEN"
            android:textAlignment="center"
            android:id="@+id/t_abdomen"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_16sdp"
            android:textColor="@color/colorAccent"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:id="@+id/l_abdomen"
            android:background="#ecf0f1"
            android:layout_below="@+id/t_abdomen">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:id="@+id/j_abdomen"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:text="Rumah Sakit"/>

        </RelativeLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/l_abdomen"
            android:text="EKSTREMITAS"
            android:textAlignment="center"
            android:id="@+id/t_ekstremitas"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_16sdp"
            android:textColor="@color/colorAccent"/>
