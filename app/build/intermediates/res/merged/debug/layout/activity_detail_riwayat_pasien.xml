            android:textAlignment="center"
            android:id="@+id/t_diagnosa"
            android:textStyle="bold"
            android:layout_marginTop="@dimen/_10sdp"
            android:textSize="@dimen/_16sdp"
            android:textColor="@color/colorAccent"/>
        <RelativeLayout
